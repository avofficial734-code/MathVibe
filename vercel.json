{
    "schema": "https://openapi.vercel.sh/vercel.json",
    "outputDirectory": "public",
    "functions": {
        "api/lambda.php": {
            "runtime": "vercel-php@8.1"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)","destination": "/api/lambda.php"
        }
    ]
}